    background-image:url(../images/bg_authorbox.jpg);
    background-repeat: repeat-x;
    background-position: bottom;
}
.authorbio .providerLink {
    margin-top: 1em;
    margin-bottom: .4em;
}
.searchtools {
    /*background-color: #f5f5f5;*/
    border-top: #eee solid 1px;
    border-bottom: #eee solid 1px;
