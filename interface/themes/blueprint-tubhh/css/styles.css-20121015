@CHARSET "UTF-8";  

/** General ***/

body {
    margin: 1.5em 0;
    background-color: #deeaef; 
}






/* responsive design */

@media (min-device-width: 1290px) {
    body {
    background-image: url('http://www.tub.tu-harburg.de/wp-content/themes/TUBHH2-twentyeleven/images/bg/tuhh-bg-2000-60-gauss-1.jpg') !important;
    background-color: #2f484f !important;
    background-repeat: no-repeat !important;
    background-position: top center !important;.
    background-attachment: scroll !important;
    /* background:url("../images/bg/DSC_8555a-2000x4.jpg") no-repeat scroll center top !important;
    background-color: #a59990 !important; */
    }
}

@media (max-width: 1280px) {
    body {
    background:none !important;
    background-color: #2F484F !important;
    }
}

@media (max-width: 800px) {
    body {
    background:none !important;
    background-color: #2F484F !important;
    }
}
@media (max-width: 650px) {
    body {
    background:none !important;
    background-color: #2F484F !important;
    }
}



/* adjust some default blueprint styles */
p,ul,dl,form,input,textarea,select,label { margin-bottom: 0.2em; }
h1 { font-size: 1.5em; line-height:1.3em; margin-top: 0.5em; margin-bottom: 0.5em; }
h2 { font-size: 1.4em; margin-top: 0.5em; margin-bottom: 0.5em; }
h3 { font-size: 1.3em; margin-top: 0.5em; margin-bottom: 0.5em; }
h4 { font-size: 1.2em; margin-top: 0.5em; margin-bottom: 0.5em; }
h5 { font-size: 1.1em; font-weight: bold; margin-bottom: 0.5em; }
h6 { font-size: 1em; font-weight: bold; margin-bottom: 0.4em; }
fieldset { padding:0 .4em .8em 1em;}
legend { font-size:100%;}
fieldset, #IE8#HACK { padding-top:0.4em; }

.container {
    /* -moz-border-radius: 8px 8px 8px 8px; */
    /* border: 1px solid #ddd; */
    /* background: none repeat scroll 0 0 #FFFFFF; */
}

.header {
    background-color: none;
    margin-bottom:3em;
}


.main {
    background-color:#fff;
    margin-top:0em;
    display:block;
}

.footer {
    background-color:#fff;
    margin-top:3em
    
}

.searchbox {
    margin-top:0em;
    padding-left:0.8em;
    padding-right:0.8em;
    padding-top:3em;
    padding-bottom:2em;
}

.searchbox,.breadcrumbs,.main,.footer {
    /** border-top: 1px solid #ddd; */
    background-color:#fff;
}


.breadcrumbs,.main,.footer {
    padding: 0.8em;
}


.clearer {
    clear: both;
}

#logo {
    background:url("../images/tubfind_logo.jpg") no-repeat scroll left center transparent;
    float:left;
    height:50px;
    width:150px;
}

#headerRight {
    /*TUHH Logo*/ 
    width:195px;
    height:50px;
    float: right;
    text-align: right;
}

#langForm {
    margin-top: .3em;
}

label.invalid {
    background:url("../images/unchecked.gif") no-repeat scroll 0 0 transparent;
    color:#EA5200;
    font-weight:bold;
    padding-bottom:2px;
    padding-left:16px;
    margin-left: 5px;
}

/** class to make stuff "invisible" but still readable by screen readers */
.offscreen {
    position:absolute;
    left:-10000px;
    top:auto;
    width:1px;
    height:1px;
    overflow:hidden;
    display:inline;
}

div.dialogLoading {
    background: url(../images/dialog_loading.gif) no-repeat center center;
    height:100%;
}

.ajax_availability {
    background: url(../images/ajax_loading.gif) no-repeat left top;
    padding:0 .5em .5em 20px;
}
.highlight {
    font-weight: bold;
    background: none;
}

.listtitle {
   font-size: 120%;
}

label {
    font-size: 100%;
    font-weight:normal;
}

select {
    font-size: 100%;
    font-weight:normal;
}

input {
    font-size: 100%;
    font-weight:normal;
}
        


.floatleft { float:left; }
.floatright { float:right; }
img.alignright { 
    float: right;
    margin: 0 0 0 10px;
}    
img.alignleft {
    float: left;
    margin: 0 10px 0 0;
}

p.listDescription {
    padding: .4em;
}

/** Buttons to do "bulk" actions **/
div.bulkActionButtons input.button {
    background-color:transparent;
    background-position: 2px center;
    text-decoration: underline;
    border:0 none;
    cursor:pointer;
    padding:0 0 0 23px;
    vertical-align: top;
    color: #06C;
}

div.bulkActionButtons input.button {
    width: 0;  /* IE table-cell margin fix */
    overflow: visible;
}

div.bulkActionButtons input.button[class] {  /* IE < 7 ignores [class] */
    width: auto;  /* cancel margin fix for other browsers */
}

div.bulkActionButtons input.checkbox {
    vertical-align: top;
}

input.smallButton {
    background-position: 2px center;
    padding: 2px 2px 2px 25px;
}

/** Cart **/
#cartSummary {
    float:right;
    width: 90px;
    padding: 2px;
    text-align: right;
}
ul.cartContent {
    list-style-type:none;
    margin:0;
    padding:0;
}

/** Alphabetic browse **/
.alphaBrowseResult {
    padding: 0 0.5em 0.5em 0.5em;
}

.alphaBrowseEntry {
    padding: 0.25em 0.5em;
    background-color: #eee;
    zoom: 1;
}

.alphaBrowseEntry.alt, .alphaBrowseEntry.alt * {
    background-color: #fff;
}

.alphaBrowseForm {
    zoom: 1;
}

.alphaBrowseHeading {
    float: left;
    width: 650px;
    line-height: 1.31em;
}

.alphaBrowseCount {
    float: right;
}

.alphaBrowseHeader {
    text-align: right;
    clear: both;
    font-weight: bold;
    zoom: 1;
}

.alphaBrowsePageLinks {
    margin: 1em;
    font-weight: bold;
    background-color: #fff;
}

.alphaBrowsePrevLink {
    float: left;
}

.alphaBrowseNextLink {
    float: right;
}

.alphaBrowseRelatedHeading {
    clear: left;
}

.alphaBrowseRelatedHeading .title {
    font-weight: bold;
    font-size: 90%;
    margin-left: 2em;
}

.alphaBrowseRelatedHeading li {
    margin-left: 4em;
}

/** Autocomplete */
.ui-autocomplete { 
    max-width: 500px; 
    max-height: 300px;
    overflow: hidden;
}

/** Publication Year facet */
fieldset.pubyearLimit {
    padding: 0;
    border: 0;
}
fieldset.publishDateLimit .ui-slider {
    clear: both; 
    margin: .5em;
}
    

/*****************Breadcrumbs*******************/
div.breadcrumbs {
    overflow:hidden;
    padding:0;
    height:30px;
    line-height:30px;
    border-top: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
    /*background-color:#eaf1eb; */
    background-color:#fff; 
}
div.breadcrumbs a, div.breadcrumbs em, div.breadcrumbs span {
    float:left;
    overflow:hidden;
    height:30px;
    padding:0 8px;
    font-style:normal;
    font-size: 90%;
}
div.breadcrumbs span {
    background:url(../images/bg_breadcrumb.png) no-repeat left center;
    overflow:hidden;
    padding:0;
    width:10px;
    font-size: 0pt;
    line-height: 0px;

    filter:alpha(opacity=40);
    opacity:0.4;
}
div.breadcrumbinner {
    margin:0 .4em;
}

/*****************Homepage*******************/
.searchHomeContent {
    padding:5em 0 ;
    /* margin:0 auto; */
    margin-top:3em;
    width:800px;
    text-align:left;
    background-color:#fff;
}
.searchHomeForm {
    background-color: #fff;
    padding:2em;
    text-align:left;
    margin-top:.5em;
    font-size: 12pt;
    -moz-border-radius: 2px 2px 2px 2px;
    /* border: 2px solid #C6D880; */
    /* color: #264409; */
}
div.searchHomeForm div.searchform {
    /*width:600px;*/
    width:100%;
    margin-left: auto;
    margin-right: auto;
}

.searchHomeForm a, .searchHomeForm a:hover {
    color:#264409;
}
.searchHomeBrowse {
    margin-left:auto;
    margin-right:auto;
    padding: .4em;
    width:800px;
}
.searchHomeBrowse h2 {
    /* border-bottom: 1px solid #94c632; */
}

.searchHomeBrowse ul {
    list-style-type: none;
    padding:0;
}

/******** Result List ***/

ul.recordSet {
    margin: 0;
    padding: 0;
    list-style-type: none;
}
ul.recordSet li { /*Abstand in Kurzliste vom Rand größer*/
    padding: 1.0em;
}


